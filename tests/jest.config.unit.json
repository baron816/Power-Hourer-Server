{
  "testEnvironment": "jest-environment-node",
  "roots": [
    "../src"
  ],
  "setupTestFrameworkScriptFile": "../src/utils/test_setup.js",
  "testPathIgnorePatterns": [
    "/helpers/",
    "/fixtures/"
  ],
  "collectCoverageFrom": [
    "src/**"
  ],
  "coverageDirectory": "coverage/integration",
  "coverageThreshold": {
    "global": {
      "statements": 0,
      "branches": 0,
      "functions": 0,
      "lines": 0
    }
  }
}
