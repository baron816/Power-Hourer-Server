{
  "name": "power-hourer-server",
  "version": "1.0.0",
  "description": "Backend for making power hours",
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon ./src/index.js --exec babel-node .babelrc",
    "clean": "rm -rf dist && mkdir dist",
    "build-server": "babel -d ./dist ./src -s",
    "build": "npm run clean && npm run build-server",
    "start": "node ./dist/index.js",
    "test": "NODE_ENV=testing mocha --compilers js:babel-core/register ./src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "power-hourer-server"
  },
  "author": "baron816",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "google-auth-library": "^0.10.0",
    "mongoose": "^4.9.6",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "mockgoose": "^7.3.1",
    "supertest": "^3.0.0"
  }
}
